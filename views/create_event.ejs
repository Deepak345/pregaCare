                    <h3 class="page-title">New Event</h3>
                    <div class="row mb-2">
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-body">

                <form class="forms-sample" id="eve-form">
                    <div class="row">
                        <div class="form-group col-sm-12 col-xs-12 col-md-6 col-lg-6">
                            <label for="exampleInputEmail1">Event Name:</label>
                            <input type="text" class="form-control p-input" id="ename">
                        </div>
                        <div class="form-group col-sm-12 col-xs-12 col-md-6 col-lg-6">
                            <label for="exampleInputEmail1">Venue</label>
                            <input type="text" class="form-control p-input" id="venue">
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                            <label for="exampleInputEmail1">Event Description</label>
                            <textarea type="text" rows="10" class="form-control p-input" id="content"></textarea>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-sm-12 col-xs-12 col-md-3 col-lg-3">
                            <label for="exampleInputEmail1">Start Date</label>
                            <input type="date" rows="10" class="form-control p-input" id="sdate">
                        </div>
                        <div class="form-group col-sm-12 col-xs-12 col-md-3 col-lg-3">
                            <label for="exampleInputEmail1">Start Time</label>
                            <input type="time" rows="10" class="form-control p-input" id="stime">
                        </div>
                        <div class="form-group col-sm-12 col-xs-12 col-md-3 col-lg-3">
                            <label for="exampleInputEmail1">End Date</label>
                            <input type="date" rows="10" class="form-control p-input" id="edate">
                        </div>
                        <div class="form-group col-sm-12 col-xs-12 col-md-3 col-lg-3">
                            <label for="exampleInputEmail1">End Time</label>
                            <input type="time" rows="10" class="form-control p-input" id="etime">                        </div>
                    </div>
                    <div class="row">
                        <div class="col-12">
                            <button type="submit" class="btn btn-primary" id="submit">SUBMIT</button>
                        </div>
                    </div>

                </form>
